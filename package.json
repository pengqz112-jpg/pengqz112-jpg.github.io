{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "hexo generate",
    "clean": "hexo clean",
    "deploy": "hexo deploy",
    "server": "hexo server --watch",
    "dev": "hexo server --watch",
    "watch": "node auto-push.js",
    "regenerate": "node auto-regenerate.js",
    "dev:auto": "concurrently \"hexo server\" \"node auto-regenerate.js\" \"node auto-push.js\""
  },
  "hexo": {
    "version": "8.1.1"
  },
  "dependencies": {
    "hexo": "^8.0.0",
    "hexo-generator-archive": "^2.0.0",
    "hexo-generator-category": "^2.0.0",
    "hexo-generator-index": "^4.0.0",
    "hexo-generator-tag": "^2.0.0",
    "hexo-renderer-ejs": "^2.0.0",
    "hexo-renderer-kramed": "^0.1.4",
    "hexo-renderer-stylus": "^3.0.1",
    "hexo-server": "^3.0.0",
    "hexo-theme-landscape": "^1.0.0",
    "hexo-theme-next": "^8.27.0"
  },
  "devDependencies": {
    "chokidar": "^5.0.0",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1"
  }
}
